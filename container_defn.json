{
    "family": "node-task-ec2", 
    "networkMode": "Bridge", 
    "executionRoleArn": "arn:aws:iam::368355641188:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "node-container", 
            "image": "368355641188.dkr.ecr.us-east-1.amazonaws.com/node-app:tag_value", 
            "portMappings": [
                {
                    "containerPort": 8000, 
                    "hostPort": 8000, 
                    "protocol": "http"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "EC2"
    ], 
    "cpu": "256", 
    "memory": "512"
}
